{
  "expo": {
    "name": "forge-gen",
    "slug": "forge-gen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#03143E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nathansomto.forgegen"
    },
    "android": {
      "package": "com.nathansomto.forgegen"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Medium.ttf",
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/images"]
        }
      ]
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09ded067-3340-4c35-ad0c-e2b7c76a96ff"
      }
    },
    "experiments": {
      "tsconfigpaths": true
    }
  }
}
